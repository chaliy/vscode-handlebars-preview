{
  "name": "handlebars-preview",
  "displayName": "Handlebars Preview",
  "description": "Live preview for Handlebar templates",
  "version": "1.2.0",
  "publisher": "chaliy",
  "license": "MIT",
  "engines": {
    "vscode": "^1.69.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaliy/vscode-handlebars-preview"
  },
  "galleryBanner": {
    "color": "#0F6378",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Handlebars",
    "Live Preview"
  ],
  "activationEvents": [
    "onCommand:handlebars.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "handlebars.preview",
        "title": "Handlebars: Open Preview"
      }
    ],
    "keybindings": [
      {
        "command": "handlebars.preview",
        "key": "ctrl+p h",
        "when": "!terminalFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}
